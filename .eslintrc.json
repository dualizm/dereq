{
    "root": true,
    "env": {
        "jest/globals": true
    },
    "extends": [
        "eslint:recommended"
        ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-var": "error",
        "semi": "error",
        "indent": "error",
        "no-multi-spaces": "error",
        "space-in-parens": "error",
        "no-multiple-empty-lines": "error",
        "prefer-const": "error",
        "no-use-before-define": "error",
        "react/prop-types": "off",
        "no-unused-vars": "off",
        "no-unused-labels": "off"
    }
}